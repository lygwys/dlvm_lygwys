@model Mzg.Web.Models.LogDetailModel

<div class="panel panel-default">
    <div class="panel-heading">
        <div class="panel-title">
            <strong>@app.PrivilegeTree?.LastOrDefault().DisplayName</strong>
        </div>
    </div>
    <div id="collapseTwo" class="panel-collapse collapse in">
        <div class="panel-body">
            <div class="form-group col-sm-12">
                <label class="col-sm-2 control-label">@app.T["log_title"]</label>
                <div class="col-sm-10">
                    @Model.LogDetail.Title
                </div>
            </div>
            <div class="form-group col-sm-12">
                <label class="col-sm-2 control-label">@app.T["log_clientip"]</label>
                <div class="col-sm-10">
                    @Model.LogDetail.ClientIP
                </div>
            </div>
            <div class="form-group col-sm-12">
                <label class="col-sm-2 control-label">@app.T["user"]</label>
                <div class="col-sm-10">
                    @Model.LogDetail.UserName
                </div>
            </div>
            <div class="form-group col-sm-12">
                <label class="col-sm-2 control-label">@app.T["link"]</label>
                <div class="col-sm-10">
                    @Model.LogDetail.Url
                </div>
            </div>
            <div class="form-group col-sm-12">
                <label class="col-sm-2 control-label">@app.T["log_urlreferrer"]</label>
                <div class="col-sm-10">
                    @Model.LogDetail.UrlReferrer
                </div>
            </div>
            <div class="form-group col-sm-12">
                <label class="col-sm-2 control-label">@app.T["operation_time"]</label>
                <div class="col-sm-10">
                    @Model.LogDetail.CreatedOn
                </div>
            </div>
            <div class="form-group col-sm-12">
                <label class="col-sm-2 control-label">@app.T["log_statuscode"]</label>
                <div class="col-sm-10">
                    @Model.LogDetail.StatusCode
                </div>
            </div>
            <div class="form-group col-sm-12">
                <label class="col-sm-2 control-label">@app.T["description"]</label>
                <div class="col-sm-10">
                    @Model.LogDetail.Description
                </div>
            </div>
        </div>
    </div>
</div>